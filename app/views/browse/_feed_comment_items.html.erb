<blockquote class="comments" id="<%= feed_comment_item.id %>">
	<h5><%= link_to feed_comment_item.user.user_name, feed_comment_item.user %></p>
	<small><%= feed_comment_item.content %></small>
	<h6><%= time_ago_in_words(feed_comment_item.created_at) %> |
		<% if current_user?(feed_comment_item.user) %>
			, <%= link_to "", micropost_path(feed_comment_item), :method => :delete, data: { confirm: "You sure?" }, :class=>"icon-trash" %>
		<% end %>
	</h6>
</blockquote>