<blockquote class="posts" id="<%= feed_item.id %>">
	<h5><%= link_to feed_item.user.user_name, feed_item.user %>, <%= time_ago_in_words(feed_item.created_at) %></h5>
	<p><%= feed_item.content %></p>
	<h6>like, dislike, comment, 
		<% if current_user?(feed_item.user) %>
			<%= link_to "delete", micropost_path(feed_item), :method => :delete, data: { confirm: "You sure?" } %>
		<% end %>
	</h6>
</blockquote>