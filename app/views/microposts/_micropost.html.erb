<blockquote class="posts">
	<h5><%= @micropost.user.user_name %>, <%= time_ago_in_words(micropost.created_at) %></h5>
	<p><%= @micropost.content %></p>
	<h6>like, dislike, comment, <% if current_user?(@micropost.user) %>
		<%= link_to "delete", @micropost, method: :delete, data: { confirm: "You sure?" } %>
	<% end %></h6>
</blockquote>

