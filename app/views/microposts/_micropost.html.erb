<blockquote class="posts">
	<h5><%= time_ago_in_words(micropost.created_at) %></h5>
	<p><%= micropost.content %></p>
	<h6>like, dislike, comment, 
		<% if current_user?(micropost) %>
			<%= link_to "delete", micropost_path(micropost), :method => :delete, data: { confirm: "You sure?" } %>
		<% end %>
	</h6>
</blockquote>

